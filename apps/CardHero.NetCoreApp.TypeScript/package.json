{
  "name": "card-hero.net-core-app.type-script",
  "version": "0.1.0",
  "private": true,
  "sideEffects": false,
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "1.2.25",
    "@fortawesome/free-brands-svg-icons": "5.11.2",
    "@fortawesome/free-solid-svg-icons": "5.11.2",
    "@fortawesome/react-fontawesome": "0.1.7",
    "bootstrap": "4.4.1",
    "chart.js": "2.9.3",
    "dayjs": "1.8.17",
    "jquery": "3.4.1",
    "popper.js": "1.16.0",
    "react": "16.12.0",
    "react-bootstrap": "1.0.0-beta.16",
    "react-chartjs-2": "2.8.0",
    "react-dnd": "9.5.1",
    "react-dnd-html5-backend": "9.5.1",
    "react-dom": "16.12.0",
    "react-router-dom": "5.1.2"
  },
  "devDependencies": {
    "@babel/core": "7.7.4",
    "@babel/plugin-transform-react-jsx": "7.7.4",
    "@types/chart.js": "2.9.3",
    "@types/jest": "24.0.23",
    "@types/node": "12.12.14",
    "@types/react": "16.9.15",
    "@types/react-dom": "16.9.4",
    "@types/react-router-dom": "5.1.3",
    "@types/react-test-renderer": "16.9.1",
    "@typescript-eslint/eslint-plugin": "2.10.0",
    "@typescript-eslint/parser": "2.10.0",
    "babel-eslint": "10.0.3",
    "babel-jest": "24.9.0",
    "babel-preset-react-app": "9.1.0",
    "cache-loader": "4.1.0",
    "clean-webpack-plugin": "3.0.0",
    "cross-env": "6.0.3",
    "css-loader": "3.2.1",
    "eslint": "6.7.2",
    "eslint-config-react-app": "5.1.0",
    "eslint-loader": "3.0.3",
    "eslint-plugin-flowtype": "4.5.2",
    "eslint-plugin-import": "2.18.2",
    "eslint-plugin-jsx-a11y": "6.2.3",
    "eslint-plugin-react": "7.17.0",
    "eslint-plugin-react-hooks": "2.3.0",
    "jest": "24.9.0",
    "jest-environment-jsdom-fourteen": "1.0.1",
    "jest-watch-typeahead": "0.4.2",
    "mini-css-extract-plugin": "0.8.0",
    "mockdate": "2.0.5",
    "prettier": "1.19.1",
    "prettier-webpack-plugin": "1.2.0",
    "react-app-polyfill": "1.0.5",
    "react-test-renderer": "16.12.0",
    "sass": "1.23.7",
    "sass-loader": "8.0.0",
    "source-map-loader": "0.2.4",
    "terser-webpack-plugin": "2.2.2",
    "ts-loader": "6.2.1",
    "typescript": "3.7.3",
    "webpack": "4.41.2",
    "webpack-bundle-analyzer": "3.6.0",
    "webpack-cli": "3.3.10",
    "webpack-deep-scope-plugin": "1.7.0",
    "webpack-manifest-plugin": "2.2.0"
  },
  "scripts": {
    "analyse": "cross-env NODE_ENV=development CH_ANALYSE=true webpack --config webpack.config.js",
    "analyse:prod": "cross-env NODE_ENV=production CH_ANALYSE=true webpack --config webpack.config.js",
    "build": "cross-env NODE_ENV=development webpack --config webpack.config.js",
    "build:prod": "cross-env NODE_ENV=production webpack --config webpack.config.js",
    "test": "cross-env CI=true node scripts/test.js --env=jsdom",
    "lint": "eslint ./ClientApp/ --cache --ext .ts,.tsx",
    "watch": "cross-env NODE_ENV=development webpack --watch --info-verbosity verbose"
  },
  "jest": {
    "roots": [
      "<rootDir>/ClientApp"
    ],
    "collectCoverageFrom": [
      "<rootDir>/ClientApp/**/*.{js,jsx,ts,tsx}",
      "!<rootDir>/ClientApp/**/*.d.ts",
      "!<rootDir>/ClientApp/**/node_modules/**"
    ],
    "coverageDirectory": "./obj/coverage/js",
    "collectCoverage": true,
    "setupFiles": [
      "react-app-polyfill/jsdom"
    ],
    "setupFilesAfterEnv": [],
    "testMatch": [
      "<rootDir>/ClientApp/**/__tests__/**/*.{js,jsx,ts,tsx}",
      "<rootDir>/ClientApp/**/*.{spec,test}.{js,jsx,ts,tsx}"
    ],
    "testEnvironment": "jest-environment-jsdom-fourteen",
    "transform": {
      "^.+\\.(js|jsx|ts|tsx)$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|ts|tsx|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx|ts|tsx)$",
      "^.+\\.module\\.(css|sass|scss)$"
    ],
    "modulePaths": [],
    "moduleNameMapper": {
      "^react-native$": "react-native-web",
      "^.+\\.module\\.(css|sass|scss)$": "identity-obj-proxy"
    },
    "moduleFileExtensions": [
      "web.js",
      "js",
      "web.ts",
      "ts",
      "web.tsx",
      "tsx",
      "json",
      "web.jsx",
      "jsx",
      "node"
    ],
    "watchPlugins": [
      "jest-watch-typeahead/filename",
      "jest-watch-typeahead/testname"
    ]
  },
  "babel": {
    "presets": [
      "react-app"
    ]
  }
}
